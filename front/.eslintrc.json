{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "no-console": 0,
      "max-len": ["error", { "code": 120 }],
      "react/react-in-jsx-scope" : 0,
      "react/jsx-one-expression-per-line": 0
    }
}
